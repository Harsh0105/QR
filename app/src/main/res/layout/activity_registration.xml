<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistrationAct">

    <EditText
        android:id="@+id/ET2"
        android:layout_width="236dp"
        android:layout_height="44dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:fontFamily="@font/aldrich"
        android:hint="201xxxxxxx"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/PT5"
        app:layout_constraintTop_toBottomOf="@+id/PT5" />

    <TextView
        android:id="@+id/PT4"
        android:layout_width="196dp"
        android:layout_height="71dp"
        android:fontFamily="@font/bungee_inline"
        android:text="SIGN UP"
        android:textAlignment="center"
        android:textColor="#CD2525"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.479"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/PT5"
        android:layout_width="241dp"
        android:layout_height="30dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/almendra_bold"
        android:text="Registration Number"
        android:textColor="#2196F3"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PT4" />

    <TextView
        android:id="@+id/PT6"
        android:layout_width="237dp"
        android:layout_height="27dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/almendra_bold"
        android:text="Email Id"
        android:textColor="#2196F3"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="@+id/ET2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ET2"
        app:layout_constraintTop_toBottomOf="@+id/ET2" />

    <EditText
        android:id="@+id/Email1"
        android:layout_width="385dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:fontFamily="@font/aldrich"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="@+id/PT6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/PT6"
        app:layout_constraintTop_toBottomOf="@+id/PT6" />

    <TextView
        android:id="@+id/PT7"
        android:layout_width="244dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/almendra_bold"
        android:text="Password"
        android:textColor="#2196F3"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Email1"
        app:layout_constraintTop_toBottomOf="@+id/Email1" />

    <EditText
        android:id="@+id/Pass2"
        android:layout_width="369dp"
        android:layout_height="45dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.285"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PT7" />

    <Button
        android:id="@+id/B5"
        android:layout_width="200dp"
        android:layout_height="59dp"
        android:layout_marginTop="120dp"
        android:fontFamily="@font/aldrich"
        android:text="Continue"
        android:textSize="15sp"
        app:backgroundTint="#2196F3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Pass2" />

    <TextView
        android:id="@+id/PT8"
        android:layout_width="323dp"
        android:layout_height="27dp"
        android:layout_marginTop="37dp"
        android:fontFamily="@font/aldrich"
        android:text="Have An Account? Log in"
        android:textAlignment="center"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/B5"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/PT9"
        android:layout_width="245dp"
        android:layout_height="34dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/almendra_bold"
        android:text="Mobile Number"
        android:textColor="#2196F3"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.057"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Pass2" />

    <EditText
        android:id="@+id/Phone1"
        android:layout_width="370dp"
        android:layout_height="57dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.243"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PT9" />
</androidx.constraintlayout.widget.ConstraintLayout>